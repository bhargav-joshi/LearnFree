// Questiong++ e sum of them is equal to 10

#include <bits/stdc++.h>
using namespace std;

int main(){
    int array[] = {2,4,3,1,8,7,11};
    int n = sizeof(array)/sizeof(array[0]);
    sort(array,array+n);
    int last = 6;
    for (int  i = 0; i<4;)
    {
        int s = array[i];
        int e = array[last];
        if(s+e<10){
            s = array[i+1];
            i++;
        }
        else if(s+e>10){
            e = array[last-1];
            last = last-1;
        }
        else if(s+e==10){
            cout<<s<<endl;
            cout<<e<<endl;
            break;
        }
    }
    
}

